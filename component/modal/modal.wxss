.header{
  height: 44px;
  width: 100%;
  display: inline-flex;
  justify-content: space-between;
}
.header image {
  display: inline-block;
}
.header image:first-child {
  width: 18px;
  height: 20px;
}
.header image:nth-child(2) {
  width: 44px;
  height: 21px;
}
.header image:nth-child(3) {
  width: 19px;
  height: 19px;
}