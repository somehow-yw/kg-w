.haderwarp {
  width: 100%;
  height: 88px;
  display: inline-flex;
  justify-content: space-between;
}
.haderwarp:first-child {
  width: 36px;
  height: 40px;
  background-image: url("images/index/ring.png")
}
.haderwarp:nth-child(2) {
  width: 87px;
  height: 42px;
  background-image: url("images/index/kg.png")
}
.haderwarp:last-child {
  width: 38px;
  height: 38px;
  background-image: url("images/index/search.png")
}